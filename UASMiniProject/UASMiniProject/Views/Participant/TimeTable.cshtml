@model IEnumerable<UASMiniProject.Models.Offer_Scheduled>

@{
    ViewBag.Title = "TimeTable";
    Layout = "~/Views/Shared/_LayoutLogin.cshtml";
}
<br />
<br /><br />    <br />
<div class="col-lg">
    <input type="button" name="button1" value="Admin Menu" class="btn btn-danger" onclick="location.href='@Url.Action("Index", "Admin")'" />
    <input type="button" name="button2" value="List Of Report" class="btn btn-success" onclick="location.href='@Url.Action("Reports", "Admin")'" />

</div>
<div class="col-lg-offset-3">
    <h3 class="text-danger">List Of Programs Scheduled To Commence In A Given Time Period</h3>
</div>
<br /> v

<table class="table table-hover">
    <tr class="success">
        <th>@Html.DisplayNameFor(model => model.ProgramsScheduled.Scheduled_Program_Id)</th>
        <th>@Html.DisplayNameFor(model => model.ProgramsOffered.Program_Name)</th>
        <th>@Html.DisplayNameFor(model => model.ProgramsOffered.Duration_in_years)</th>
        <th>@Html.DisplayNameFor(model => model.ProgramsScheduled.Start_Date)</th>
        <th>@Html.DisplayNameFor(model => model.ProgramsScheduled.End_Date)</th>
        <th>@Html.DisplayNameFor(model => model.ProgramsScheduled.Sessions_Per_Week)</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr class="info">
            <td>
                @Html.DisplayFor(modelItem => item.ProgramsScheduled.Scheduled_Program_Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProgramsOffered.Program_Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProgramsOffered.Duration_in_years)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProgramsScheduled.Start_Date)

            <td>
                @Html.DisplayFor(modelItem => item.ProgramsScheduled.End_Date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProgramsScheduled.Sessions_Per_Week)
            </td>
        </tr>
    }
</table>